<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="main" extends="struts-default">
		<action name="home" class="vn.com.fpt.actions.LoginAction">
			<result name="home">/home.jsp</result>
		</action>
	</package>
	
	<package name="addGet" extends="struts-default">
		<action name="addGet" class="vn.com.fpt.actions.AddGetAction">
			<result name="addGet">/add.jsp</result>
		</action>
	</package>
	
	<package name="item" extends="struts-default">
		<action name="item" class="vn.com.fpt.actions.ViewItemAction">
			<result name="item">/viewItem.jsp</result>
		</action>
	</package>
	
	<package name="failed" extends="struts-default">
		<action name="failed" class="vn.com.fpt.actions.NewItemFailedAction">
			<result name="failed">/fAdd.jsp</result>
		</action>
	</package>	
</struts>